<Application x:Class="ZigIDE3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZigIDE3"
             xmlns:viewModel="clr-namespace:ZigIDE3.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="SvgDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <viewModel:CodeViewModel x:Key="AppCodeViewModel"/>
            <viewModel:MenuViewModel x:Key="AppMenuViewModel"/>

        </ResourceDictionary>
        
    </Application.Resources>
</Application>
